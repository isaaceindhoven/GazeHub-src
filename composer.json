{
    "name": "isaac/gaze-hub-src",
    "description": "The source code behind GazeHub.",
    "type": "project",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "~7.3.0 || ~7.4.0 || ~8.0.0",
        "ext-json": "*",
        "firebase/php-jwt": "^6.0",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "psr/log": "^1.1",
        "rakit/validation": "^1.4",
        "react/event-loop": "^1.1",
        "react/http": "^1.2",
        "react/socket": "^1.6",
        "react/stream": "^1.1"
    },
    "require-dev": {
        "isaac/php-code-sniffer-standard": "^21.0",
        "nunomaduro/collision": "^5.3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpstan/phpstan": "^0.12.81",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ISAAC\\GazeHub\\": "src/",
            "ISAAC\\GazeHub\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "bin": [
        "bin/gazehub"
    ]
}
