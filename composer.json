{
    "name": "isaac/gazehub",
    "description": "Server-Sent Event Hub, GazeHub",
    "type": "project",
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require": {
        "ext-json": "*",
        "firebase/php-jwt": "^5.2",
        "php-di/php-di": "^6.3",
        "php": "~7.3.0 || ~7.4.0 || ~8.0.0",
        "rakit/validation": "^1.4",
        "react/event-loop": "^1.1",
        "react/http": "^1.2",
        "react/socket": "^1.6",
        "react/stream": "^1.1"
    },
    "require-dev": {
        "isaac/composer-git-hooks-standard": "^1.3",
        "isaac/composer-git-hooks": "^1.2",
        "isaac/php-code-sniffer-standard": "^20.0",
        "nunomaduro/collision": "^5.3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpstan/phpstan": "^0.12.81",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ISAAC\\GazeHub\\": "src/",
            "ISAAC\\GazeHub\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "install-git-hooks": "ISAAC\\ComposerGitHooks\\ComposerPlugin::process"
    },
    "license": "proprietary",
    "bin": [
        "gazehub"
    ],
    "repositories": {
        "isaac": {
            "type": "composer",
            "url": "https://composer-packages.hq.isaac.nl/repository"
        }
    }
}
